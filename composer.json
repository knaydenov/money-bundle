    "type": "symfony-bundle",
    "autoload" : {
        "psr-4" : { "Kna\\MoneyBundle\\" : "" }
    },
    "autoload-dev": {
        "psr-4": {
            "Kna\\MoneyBundle\\Tests\\App\\": "Tests/App/src/"
        }
    },
